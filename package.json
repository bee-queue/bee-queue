{
  "name": "bee-queue",
  "version": "1.2.3",
  "description": "A simple, fast, robust job/task queue, backed by Redis.",
  "main": "index.js",
  "dependencies": {
    "promise-callbacks": "^3.8.1",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "ava-spec": "^1.1.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "lolex": "^2.0.0",
    "nyc": "^11.0.3",
    "sandboxed-module": "^2.0.3",
    "semver": "^7.2.1",
    "sinon": "^2.3.8"
  },
  "files": [
    "HISTORY.md",
    "README.md",
    "LICENSE",
    "package.json",
    "index.js",
    "index.d.ts",
    "lib"
  ],
  "scripts": {
    "ci": "if node -e '({...0})' 2>/dev/null; then npm run lint && npm run coverage-and-publish; else ava; fi",
    "coverage-and-publish": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc ava",
    "lint": "eslint .",
    "report": "npm run coverage && nyc report --reporter=html",
    "test": "npm run lint && ava"
  },
  "keywords": [
    "task",
    "job",
    "queue",
    "worker",
    "distributed",
    "system",
    "redis",
    "lua"
  ],
  "engines": {
    "node": ">= 4"
  },
  "author": "Lewis J Ellis <me@lewisjellis.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bee-queue/bee-queue.git"
  },
  "bugs": {
    "url": "https://github.com/bee-queue/bee-queue/issues"
  },
  "homepage": "https://github.com/bee-queue/bee-queue"
}
